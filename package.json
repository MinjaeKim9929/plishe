{
  "name": "plishe-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend/web",
    "frontend/mobile",
    "backend/api",
    "shared/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=frontend/web",
    "dev:api": "npm run dev --workspace=backend/api",
    "build:web": "npm run build --workspace=frontend/web",
    "build:api": "npm run build --workspace=backend/api"
  }
}
